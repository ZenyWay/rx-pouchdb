{
  "name": "rx-pouchdb",
  "version": "1.0.3",
  "description": "thin RXJS abstraction layer for pouchDB: read and write RXJS operators",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "aliasify": "spec/aliasify.conf.js",
  "scripts": {
    "predist": "mkdirp dist && rimraf dist/*",
    "dist": "tsc -p src --outDir dist",
    "example": "npm run example:bundle && npm run example:serve",
    "example:serve": "http-server -c-1 -i -e html -o -p 6510 spec/example",
    "preexample:bundle": "rimraf spec/example/*.js",
    "example:bundle": "browserify spec/example/index.ts -p [ tsify --project spec/example ] -t aliasify -t uglifyify --exclude pouchdb-browser --exclude '@reactivex%2Frxjs' --exclude bluebird --exclude debug -o spec/example/index.js",
    "prepublish": "npm run dist",
    "pretest": "mkdirp spec/reports && rimraf spec/reports/*",
    "test": "karma start spec/karma.conf.js --single-run",
    "test:debug": "karma start spec/karma.conf.js --no-single-run",
    "pretest:coverage": "mkdirp spec/reports/coverage && rimraf spec/reports/coverage",
    "test:coverage:serve": "http-server -c-1 -o -p 6510 spec/reports/coverage",
    "test:coverage": "karma start spec/karma-coverage.conf.js --single-run && remap-istanbul -i spec/reports/coverage/chrom*/coverage.json -o spec/reports/coverage -t html && npm run test:coverage:serve",
    "pretest:ci": "mkdirp spec/reports/coverage && rimraf spec/reports/coverage",
    "test:ci:coveralls": "cat spec/reports/coverage/lcov.info | sed -e 's/SF:\\.\\.\\//SF:/' | coveralls",
    "test:ci": "karma start spec/karma-coverage.conf.js --single-run && remap-istanbul -i spec/reports/coverage/chrom*/coverage.json -o spec/reports/coverage/lcov.info -t lcovonly",
    "test:web:serve": "http-server -c-1 -o -p 6510 spec/web",
    "pretest:web": "rimraf spec/web/*.js",
    "test:web": "browserify spec/*.spec.ts -p [ tsify --project spec ] -t aliasify -t uglifyify --exclude '@reactivex%2Frxjs' --exclude bluebird --exclude debug -o spec/web/index.spec.js && npm run test:web:serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenyway/rx-pouchdb.git"
  },
  "keywords": [
    "pouchDB",
    "rxjs",
    "read",
    "write",
    "functional",
    "operator"
  ],
  "author": {
    "name": "Stephane M. Catala",
    "email": "stephane@zenyway.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/zenyway/rx-pouchdb/issues"
  },
  "homepage": "https://github.com/zenyway/rx-pouchdb#readme",
  "dependencies": {
    "@reactivex/rxjs": "^5.0.3",
    "@types/bluebird": "^3.0.37",
    "@types/debug": "0.0.29",
    "bluebird": "^3.4.7",
    "debug": "^2.6.0",
    "tslib": "^1.5.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.40",
    "@types/node": "^7.0.0",
    "aliasify": "^2.1.0",
    "browserify": "^13.3.0",
    "browserify-istanbul": "^2.0.0",
    "coveralls": "^2.11.15",
    "cpx": "^1.5.0",
    "eslint": "^3.13.1",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.5",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "mkdirp": "^0.5.1",
    "pouchdb-browser": "^6.1.1",
    "remap-istanbul": "^0.8.4",
    "rimraf": "^2.5.4",
    "standard": "^8.6.0",
    "tsify": "^3.0.0",
    "typescript": "^2.1.4",
    "uglifyify": "^3.0.4",
    "watchify": "^3.8.0"
  }
}
